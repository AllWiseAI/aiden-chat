{
  "version": "0.0.16",
  "syncVersion": "0.0.2",
  "mcpServers": {
    "aiden-terminal": {
      "aiden_id": "aiden-terminal",
      "aiden_mcp_version": "",
      "aiden_type": "default",
      "aiden_enable": true,
      "args": [
        "run",
        "terminal.py"
      ],
      "command": "uv",
      "transport": "stdio"
    },
    "mcp-server-chart": {
      "aiden_id": "aiden-visualization-charts-server",
      "aiden_mcp_version": "",
      "aiden_type": "default",
      "aiden_enable": true,
      "command": "npx",
      "args": [
        "-y",
        "@antv/mcp-server-chart"
      ]
    },
    "aiden-search": {
      "url": "https://mcp.aidenai.io/mcp/search/sse",
      "transport": "sse",
      "aiden_type": "default",
      "aiden_enable": true,
      "aiden_id": "aiden-search"
    },
    "aiden-desktop-commander": {
      "command": "npx",
      "args": [
        "-y",
        "@wonderwhy-er/desktop-commander@0.2.2"
      ],
      "transport": "stdio",
      "aiden_type": "default",
      "aiden_enable": false,
      "aiden_mcp_version": "0.2.2",
      "aiden_id": "aiden-desktop-commander"
    },
    "aiden-fetch": {
      "aiden_id": "aiden-fetch",
      "aiden_mcp_version": "2025.09.18.1707",
      "aiden_type": "default",
      "args": [
        "allwise-mcp-fetch"
      ],
      "aiden_enable": true,
      "command": "uvx",
      "transport": "stdio"
    },
    "aiden-time": {
      "command": "uv",
      "args": [
        "run",
        "time_server.py"
      ],
      "transport": "stdio",
      "aiden_type": "default",
      "aiden_enable": true,
      "aiden_id": "aiden-time"
    },
    "aiden-sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking@0.6.2"
      ],
      "transport": "stdio",
      "aiden_type": "default",
      "aiden_enable": true,
      "aiden_mcp_version": "0.6.2",
      "aiden_id": "aiden-sequential-thinking"
    },
    "aiden-excel": {
      "command": "npx",
      "args": [
        "--yes",
        "@negokaz/excel-mcp-server@0.9.2"
      ],
      "env": {
        "EXCEL_MCP_PAGING_CELLS_LIMIT": "4000"
      },
      "transport": "stdio",
      "aiden_type": "default",
      "aiden_enable": true,
      "aiden_mcp_version": "0.9.2",
      "aiden_id": "aiden-excel"
    },
    "aiden-ppt": {
      "command": "uvx",
      "args": [
        "--from",
        "office-powerpoint-mcp-server@1.0.0",
        "ppt_mcp_server"
      ],
      "transport": "stdio",
      "aiden_type": "default",
      "aiden_enable": true,
      "aiden_mcp_version": "1.0.0",
      "aiden_id": "aiden-ppt"
    },
    "aiden-markitdown": {
      "command": "uvx",
      "args": [
        "markitdown-mcp@0.0.1a4"
      ],
      "transport": "stdio",
      "aiden_type": "default",
      "aiden_enable": true,
      "aiden_mcp_version": "0.0.1a4",
      "aiden_id": "aiden-markitdown"
    }
  },
  "a2aServers": []
}
