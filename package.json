{
  "name": "aidenchat",
  "private": false,
  "license": "mit",
  "scripts": {
    "dev": "concurrently -r \"next dev\"",
    "build": "cross-env BUILD_MODE=standalone next build",
    "start": "next start",
    "lint": "next lint",
    "export": "cross-env BUILD_MODE=export BUILD_APP=1 next build",
    "export:dev": "concurrently -r \"cross-env BUILD_MODE=export BUILD_APP=1 rm -rf .next && next dev -p 3000 -H 127.0.0.1\"",
    "app:dev": "sh ./update_tauri_config.sh --mode dev && concurrently -r  \"yarn tauri dev\"",
    "app:build": "sh ./update_tauri_config.sh --mode release && yarn tauri build",
    "app:build:intel": "sh ./update_tauri_config.sh --mode release && yarn tauri build --target x86_64-apple-darwin",
    "app:build:arch": "sh ./update_tauri_config.sh --mode release && yarn tauri build --target   aarch64-apple-darwin",
    "app:clear": "yarn tauri dev",
    "prepare": "husky install",
    "test": "node --no-warnings --experimental-vm-modules $(yarn bin jest) --watch",
    "test:ci": "node --no-warnings --experimental-vm-modules $(yarn bin jest) --ci"
  },
